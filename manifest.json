{
  "name": "Nutrilab",
  "version": "1.0",
  "description": "Fail Army",
  "manifest_version": 2,
  "permissions": ["declarativeContent", "storage"],
  "background": {
    "scripts": ["scripts/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "views/popup.html",
    "default_icon": {
      "256": "images/user-male.png"
    },
    "default_title": "nutrilab"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "scripts/jquery.js",
        "scripts/content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
